import { newSpecPage } from '@stencil/core/testing';
import { RuxPopup } from '../rux-popup';

describe('rux-popup', () => {
  it('renders', async () => {
    const page = await newSpecPage({
      components: [RuxPopup],
      html: `<rux-popup></rux-popup>`,
    });
    expect(page.root).toEqualHtml(`
      <rux-popup>
        <mock:shadow-root>
          <slot></slot>
        </mock:shadow-root>
      </rux-popup>
    `);
  });
});
